    "@amplitude/eslint-plugin-amplitude": "^1.0.1",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.4.1",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1"
